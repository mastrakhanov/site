{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@admin/*": [
        "src/app/admin/*"
      ]
    },
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
